{
  "cli": {
    "version": ">= 10.1.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "ios": {
        "image": "latest"
      }
    },
    "production-prebuild": {
      "ios": {
        "image": "latest"
      },
      "prebuildCommand": "npx expo prebuild --clean"
    },
    "local": {
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
